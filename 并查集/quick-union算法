#include <iostream>
using namespace std;
class unionset
{
public:
    int* father;
    int n;
    unionset(int n)
    {
        father = new int[n + 1];
        this->n = n;
        for(int i = 0;i <= n;++i)
        {
            father[i] = i;
        }
    }
    int find(int x)
    {
        if(father[x] == x) return x;
        return father[x];
    }
    void merge(int a, int b)
    {
        fa = find(a);
        fb = find(b);
        if(fa == fb) return;
        father[fa] =f b;
        return;
    }
};
int main()
{
    return 0;
}
