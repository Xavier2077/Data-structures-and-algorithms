#include <iostream>
using namespace std;

class unionset
{
private:
  int n;
  int* color;
  unionset(int n)
  {
    this->n = n;
    color = new int[n];
    for(int i = 0;i < n;++i)
      {
        color[i] = i;
      }
  }
  int find(int x)
  {
    return color[x];
  }
  void merge(int a, int b)
  {
    int cb = color[b];
    for(int i = 0;i < n;++i)
      {
        if(color[i] == cb) color[i] = color[a];
      }
    return;
  }
};

int main()
{

  return 0;
}
